# Database Configuration
DB_TYPE=postgresdb
DB_POSTGRESDB_HOST=your-database-host.com
DB_POSTGRESDB_PORT=5432
DB_POSTGRESDB_DATABASE=your_database_name
DB_POSTGRESDB_USER=your_database_user
DB_POSTGRESDB_PASSWORD=your_secure_password
DB_POSTGRESDB_SCHEMA=public

# Database SSL Configuration
DB_POSTGRESDB_SSL_REJECT_UNAUTHORIZED=false
DB_POSTGRESDB_SSL_REQUIRE=true

# Timezone Configuration
GENERIC_TIMEZONE=Asia/Kolkata
TZ=Asia/Kolkata

# n8n Encryption Key (generate a random string)
N8N_ENCRYPTION_KEY=your_random_encryption_key_here
